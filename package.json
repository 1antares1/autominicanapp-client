{
    "name": "autominicanapp-client",
    "version": "0.0.1",
    "author": "Ionic Framework",
    "homepage": "https://ionicframework.com/",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "start:ssl": "ng serve --ssl 1 --ssl-key \"src/server/certs/appdominicanapp.com.do.key.pem\" --ssl-cert \"src/server/certs/appdominicanapp.com.do.key.pem/appdominicanapp.com.do.cert.pem\"",
        "start:hmr": "ng serve --configuration hmr",
        "start:hmr:ssl": "ng serve --configuration hmr --ssl 1 --ssl-key \"src/server/certs/appdominicanapp.com.do.key.pem\" --ssl-cert \"src/server/certs/appdominicanapp.com.do.key.pem/appdominicanapp.com.do.cert.pem\"",
        "build:i18n": "ng xi18n --output-path src/i18n --outFile resources.en.xlf",
        "build:full": "npm run clean:dist && npm run build:server && npm run build",
        "build:server": "tsc --project src/server/tsconfig.server.json --sourceMap",
        "clean:dist": "npm run rimraf -- dist",
        "clean:install": "npm set progress=false && npm install",
        "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist compiled dll",
        "cls": "clear && printf '\\e[3J'",
        "install:prod": "npm update && npm install --production && node node_modules/node-sass/scripts/install.js && npm rebuild node-sass",
        "docker": "docker",
        "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
        "docs:compodoc": "compodoc -p tsconfig.json",
        "docs:compodoc:serve": "compodoc -p tsconfig.json -s",
        "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w",
        "lodash": "4.17.15",
        "node": "node",
        "node:killprocesses": "kikall node",
        "postversion": "git push && git push --tags",
        "rimraf": "rimraf",
        "server:prod": "set NODE_ENV=production && node src/server/app.js",
        "server-start": "node src/server/app.js",
        "tslint": "tslint",
        "analyzer": "webpack-bundle-analyzer dist/stats.json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "8.2.14",
        "@angular/common": "8.2.14",
        "@angular/core": "8.2.14",
        "@angular/fire": "5.2.3",
        "@angular/forms": "8.2.14",
        "@angular/platform-browser": "8.2.14",
        "@angular/platform-browser-dynamic": "8.2.14",
        "@angular/router": "8.2.14",
        "@capacitor/android": "1.4.0",
        "@capacitor/core": "1.4.0",
        "@ionic-native/core": "5.19.0",
        "@ionic-native/splash-screen": "5.19.0",
        "@ionic-native/status-bar": "5.19.0",
        "@ionic/angular": "4.11.7",
        "@ionic/pro": "2.0.4",
        "firebase": "7.6.1",
        "firebase-functions": "3.3.0",
        "core-js": "3.6.1",
        "rxjs": "6.5.4",
        "tslib": "1.10.0",
        "zone.js": "0.10.2",
        "@angular/http": "7.2.15",
        "@capacitor/cli": "1.4.0",
        "@ionic/schematics-angular": "1.0.7",
        "cordova-android": "8.1.0",
        "cordova-fabric-plugin": "1.1.14-dev",
        "cordova-ios": "5.1.1",
        "cordova-plugin-device": "2.0.3",
        "cordova-plugin-ionic-keyboard": "2.2.0",
        "cordova-plugin-ionic-webview": "4.1.3",
        "cordova-plugin-splashscreen": "5.0.3",
        "cordova-plugin-whitelist": "1.3.4",
        "ionicons": "4.6.3",
        "moment": "2.24.0",
        "ngx-pica": "1.1.4",
        "rxjs-compat": "6.5.4"
    },
    "devDependencies": {
        "@angular-devkit/architect": "0.803.21",
        "@angular-devkit/build-angular": "0.803.21",
        "@angular-devkit/core": "8.3.21",
        "@angular-devkit/schematics": "8.3.21",
        "@angular/cli": "8.3.21",
        "@angular/compiler": "8.2.14",
        "@angular/compiler-cli": "8.2.14",
        "@angular/language-service": "8.2.14",
        "@capacitor/cli": "1.4.0",
        "@ionic/angular-toolkit": "2.1.1",
        "@ionic/lab": "2.0.18",
        "@types/jasmine": "3.5.0",
        "@types/jasminewd2": "2.0.8",
        "@types/node": "13.1.2",
        "codelyzer": "5.2.1",
        "jasmine-core": "3.5.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "4.4.1",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "2.1.1",
        "karma-jasmine": "2.0.1",
        "karma-jasmine-html-reporter": "1.5.1",
        "protractor": "5.4.2",
        "ts-node": "8.5.4",
        "tslint": "5.20.1",
        "typescript": "~3.4.0",
        "@types/cordova": "0.0.34",
        "jasmine": "3.5.0"
    },
    "description": "An Ionic project"
}
